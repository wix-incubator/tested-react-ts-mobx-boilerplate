{
  "name": "tested-react-ts-mobx-boilerplate",
  "version": "0.0.1",
  "private": true,
  "description": "Frontend Boilerplate with Tests, React, MobX and TypeScript",
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:wix/tested-react-ts-mobx-boilerplate.git"
  },
  "scripts": {
    "e2e": "rimraf dist && yarn build && node scripts/test-e2e.js",
    "reset": "rimraf node_modules dist && yarn && yarn test",
    "test": "karma start --single-run && yarn e2e",
    "test:browser": "karma start --single-run",
    "start": "webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 3000",
    "build": "webpack --env.prod --progress --colors"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.8",
    "@types/classnames": "^2.2.0",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.3",
    "@types/react": "^15.0.34",
    "@types/react-dom": "^15.5.1",
    "@types/react-router": "^4.0.12",
    "@types/webpack": "^3.0.0",
    "autoprefixer": "^7.1.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-env-reporter": "^1.0.13",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-typescript-preprocessor": "^0.3.1",
    "karma-webpack": "^2.0.3",
    "mobx": "^3.1.16",
    "mobx-react": "^4.2.1",
    "mobx-react-devtools": "^4.2.15",
    "mobx-react-router": "^4.0.1",
    "mocha": "^3.4.2",
    "mocha-env-reporter": "^2.0.4",
    "mocha-loader": "^1.1.1",
    "postcss": "^6.0.3",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-reporter": "^4.0.0",
    "postcss-url": "^7.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^1.3.1",
    "react-router": "^4.1.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "test-drive-react": "^0.5.0",
    "ts-loader": "^2.1.0",
    "typescript": "~2.3.3",
    "url-loader": "^0.5.7",
    "webpack": "^3.0.0",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-hot-middleware": "^2.18.0"
  },
  "testGlob": "./test/**/*.spec.ts?(x)",
  "e2eGlob": "./e2e/**/*.spec.ts"
}
